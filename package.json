{
  "name": "mocha-chai-nyc-puppeteer-example",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "postinstall": "npm run bootstrap",
    "build:test": "npm run build:test:common && npm run build:test:client && npm run build:test:server",
    "build:test:common": "cd packages/common && webpack --config webpack.test.js",
    "build:test:client": "cd packages/client && webpack --config webpack.test.js",
    "build:test:server": "cd packages/server && webpack --config webpack.test.js",
    "test": "npm run build:test && npm run nyc && npm run clean",
    "instrument": "nyc instrument ./packages/client/bundle ./packages/client/test",
    "nyc": "nyc --cache=false --reporter=cobertura --reporter=html --reporter=text mocha --timeout=25000",
    "clean": "rimraf .nyc_output"
  },
  "keywords": [],
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "cors": "2.8.5",
    "express": "4.16.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "4.2.0",
    "lerna": "^3.13.4",
    "mocha": "6.1.4",
    "mocha-headless-chrome": "2.0.2",
    "nyc": "14.1.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
